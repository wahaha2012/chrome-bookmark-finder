{
    "name": "Bookmark Finder",
    "description": "search keyword in chrome bookmarks",
    "version": "0.1.0",
    "author": "https://github.com/wahaha2012",
    "icons": {
        "128": "icons/icon128.png",
        "48": "icons/icon48.png",
        "32": "icons/icon32.png",
        "16": "icons/icon16.png"
    },

    "permissions": [
        "tabs",
        "bookmarks"
    ],

    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_popup": "index.html",
        "default_title": "bookmark finder"
    },
    "manifest_version": 2
}